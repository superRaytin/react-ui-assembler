{
  "parser": "babel-eslint",
  "extends": "eslint-config-airbnb",
  "rules": {
    "comma-dangle": [1, "never"],
    "object-curly-spacing": [0],
    "space-before-function-paren": [0],
    "space-in-parens": [2, "never"],
    "react/jsx-no-bind": [0],
    "no-unused-vars": [0]
  },
  "ecmaFeatures": {
    "experimentalObjectRestSpread": true
  },
  "globals": {
    "Babel": true,
    "jQuery": true
  }
}
